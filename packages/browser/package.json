{
	"name": "@kevintyj/pss-browser",
	"version": "1.0.0",
	"description": "Browser automation with Playwright for PSS",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check ."
	},
	"dependencies": {
		"playwright": "^1.54.1",
		"@kevintyj/pss-types": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^24.0.13",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3"
	},
	"files": [
		"dist",
		"src"
	],
	"keywords": [
		"browser",
		"playwright",
		"automation",
		"snapshots"
	],
	"author": "",
	"license": "BSD-3-Clause"
}
